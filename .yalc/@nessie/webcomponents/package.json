{
  "name": "@nessie/webcomponents",
  "version": "1.2.2",
  "description": "Nessie - Webcomponents",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/design-system-stenciljs/design-system-stenciljs.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "npm run before-build && stencil build --docs",
    "start": "npm run before-build && stencil build --dev --watch --serve --docs",
    "test": "stencil test --spec",
    "test.watch": "stencil test --spec --watch",
    "test.e2e": "stencil test --spec --e2e",
    "test.e2e.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006 --quiet -s www",
    "build-storybook": "node ./scripts/netlify-preinstall.js && stencil build && build-storybook -s www",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run storybook\"",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "format": "prettier --write .",
    "release": "npm --no-git-tag-version version",
    "before-build": "node ./scripts/list-components-scss.js && node ./scripts/copy-icons.js"
  },
  "dependencies": {
    "@stencil/core": "2.12.0"
  },
  "license": "MIT",
  "optionalDependencies": {
    "@nessie/design-tokens": "1.2.1",
    "@nessie/web-fundamentals": "1.2.1"
  },
  "publishConfig": {
    "registry": "https://nexus.topaas.ns.nl/repository/NS_DSM_NPM/"
  },
  "yalcSig": "370f2371ac12cc6d073bd275236279e8"
}
